{
    "name": "Google Search Open in New Tab",
    "description": "Google Search Open in New Tab",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
        "16": "img/app-icon-16X16.png",
        "128": "img/app-icon-128X128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "contentscript.js"
            ]
        }
    ],
    "permissions": [
        "tabs"
    ]
}